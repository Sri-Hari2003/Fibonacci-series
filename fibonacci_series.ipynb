   "source": [
    "num_terms = int(input(\"Enter the number of terms for the Fibonacci series: \"))\n",
    "\n",
    "fib_series = [0, 1]\n",
    "while len(fib_series) < num_terms:\n",
    "    next_term = fib_series[-1] + fib_series[-2]\n",
    "    fib_series.append(next_term)\n",
    "\n",
    "print(\"Fibonacci series:\")\n",
    "print(fib_series)\n"
   ]
